# Copy to .env and set values. Used for local runs and docker-compose.

# AISstream (required for worker to receive data)
AISSTREAM_API_KEY=your_api_key_here
AISSTREAM_WS_URL=wss://stream.aisstream.io/v0/stream

# Bounding box (vessels outside this area are discarded)
ZONE_NAME=My Zone
ZONE_LAT_MIN=-90
ZONE_LAT_MAX=90
ZONE_LON_MIN=-180
ZONE_LON_MAX=180

# For Docker Compose, DATABASE_URL and REDIS_URL are set in docker-compose.yml
# (hosts: db, redis). For local runs, use:
# DATABASE_URL=postgresql+asyncpg://ais_user:ais_password@localhost:5432/ais_db
# REDIS_URL=redis://localhost:6379/0

LOG_LEVEL=INFO
